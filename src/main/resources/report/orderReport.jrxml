	<parameter name="orderIdParam" class="java.lang.String"/>
	<queryString>
		<![CDATA[
SELECT OrderDetail.ItemCode, OrderDetail.OrderQTY, Item.Name, Item.UnitPrice, (Item.UnitPrice *  OrderDetail.OrderQTY) AS 'Tot'
FROM OrderDetail
INNER JOIN Item ON OrderDetail.ItemCode = Item.ItemCode
INNER JOIN Orders ON Orders.OrderID = OrderDetail.OrderID
WHERE Orders.OrderID = $P{orderIdParam};
]]>
	</queryString>
